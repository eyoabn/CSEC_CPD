x = []
for _ in range(5):
    row = list(map(int, input().split()))
    x.append(row)

ci = 0
cj = 0
c = 0

for i in range(5):
    for j in range(5):
        if x[i][j] == 1:
            ci = i
            cj = j


while ci != 2:
    if ci < 2:
        ci += 1
    else:
        ci -= 1
    c += 1

while cj != 2:
    if cj < 2:
        cj += 1
    else:
        cj -= 1
    c += 1

print(c)
